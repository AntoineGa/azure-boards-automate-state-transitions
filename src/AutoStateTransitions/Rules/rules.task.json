{
  "type": "Task",
  "rules": [
    {
      "ifChildState": "In Progress",
      "notParentStates": [ "Committed", "Removed" ],
      "setParentStateTo": "Committed",
      "allChildren": false
    },
    {
      "ifChildState": "To Do",
      "notParentStates": [ "Approved", "Committed", "New", "Removed" ],
      "setParentStateTo": "Approved",
      "allChildren": false
    },
    {
      "ifChildState": "Done",
      "notParentStates": [],
      "setParentStateTo": "Done",
      "allChildren": true
    }
  ]
}
