{
  "type": "Task",
  "rules": [
    {
      "ifChildState": "Active",
      "notParentStates": [ "Commited", "Removed" ],
      "setParentStateTo": "Commited",
      "allChildren": false
    },
    {
      "ifChildState": "New",
      "notParentStates": [ "Approved", "Commited", "New", "Removed" ],
      "setParentStateTo": "Approved",
      "allChildren": false
    },
    {
      "ifChildState": "Closed",
      "notParentStates": [],
      "setParentStateTo": "Done",
      "allChildren": true
    }
  ]
}
